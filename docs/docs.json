{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Restate",
  "description": "Restate is the simplest way to build resilient applications.",
  "colors": {
    "primary": "#4752c4",
    "light": "#7a87e1",
    "dark": "#4752c4"
  },
  "background": {
    "decoration": "gradient"
  },
  "appearance": {
    "default": "light"
  },
  "fonts": {
    "heading": {
      "family": "Rubik"
    },
    "body": {
      "family": "Manrope"
    }
  },
  "contextual": {
   "options": [
     "copy",
     "view",
     "chatgpt",
     "claude"
   ]
 },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Build",
        "groups": [
          {
            "group": "TypeScript SDK",
            "pages": [
              "develop/ts/overview",
              "develop/ts/journaling-results",
              "develop/ts/service-communication",
              "develop/ts/state",
              "develop/ts/awakeables",
              "develop/ts/durable-timers",
              "develop/ts/error-handling",
              "develop/ts/serialization"
            ]
          }
        ]
      },

      {
        "tab": "Operate",
        "pages": [
          "operate/configuration/services"
        ],
        "groups": [
          {
            "group": "Services",
            "pages": [
              "operate/configuration/services"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://restate.dev"
  },
  "navbar": {
    "links": [
      {
        "label": "Get Restate",
